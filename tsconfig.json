{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "es2019",
      "es2020.string",
      "es2020.symbol.wellknown"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true
  }
}
